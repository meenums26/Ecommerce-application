
    <section> 
        <div class="container">
        <div class="row">
            {{#each products}}
            <div class="col-md-3 p-3">
                <div class="card" style="width: 15rem; height:30rem">
                    <img src="{{this.image}}" class="card-img-top" alt="..."
                    style="height:10rem">
                    <div class="card-body">
                        <h5 class="card-title">{{this.name}}</h5>
                        <p class="card-text">{{this.company}}</p>
                        <p class="card-text">&#x20B9; {{this.prize}}/-</p>
                        <a href="#" class="btn btn-primary">Add to Cart</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        </div>
    </section>
   